# Network RPC URLs
AMOY_RPC_URL=https://polygon-amoy.g.alchemy.com/v2/your_alchemy_api_key_here
POLYGON_RPC_URL=https://polygon-rpc.com

# Alchemy API Key (get from https://www.alchemy.com/)
# Used for Amoy network access.
ALCHEMY_API_KEY=your_alchemy_api_key_here

# Private key of the deployer account (WITHOUT 0x prefix)
# IMPORTANT: Use a dedicated deployment wallet, not your main wallet.
PRIVATE_KEY=your_private_key_here

# API Key for contract verification on PolygonScan (get from https://polygonscan.com/apis)
POLYGONSCAN_API_KEY=your_polygonscan_api_key_here

# Contract configuration
# Address for the treasury
TREASURY_ADDRESS=your_treasury_address_here
